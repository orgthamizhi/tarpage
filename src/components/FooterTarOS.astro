      <!-- Features -->
      <div>
        <h4 class="text-sm font-semibold text-gray-900 mb-4">Features</h4>
        <ul class="space-y-3">
          <li><a href="/ai-agents" class="text-gray-600 hover:text-gray-900 text-sm transition-colors">AI Agents</a></li>
          <li><a href="/commerce" class="text-gray-600 hover:text-gray-900 text-sm transition-colors">Commerce</a></li>
          <li><a href="/delivery" class="text-gray-600 hover:text-gray-900 text-sm transition-colors">Delivery</a></li>
