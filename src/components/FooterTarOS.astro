          <li><a href="/chat" class="text-gray-600 hover:text-gray-900 text-sm transition-colors">Chat</a></li>
        </ul>
      </div>

      <!-- Company -->
      <div>
        <h4 class="text-sm font-semibold text-gray-900 mb-4">Company</h4>
        <ul class="space-y-3">
          <li><a href="/about" class="text-gray-600 hover:text-indigo-600 text-sm transition-colors">About</a></li>
          <li><a href="/pricing" class="text-gray-600 hover:text-indigo-600 text-sm transition-colors">Pricing</a></li>
          <li><a href="/blog" class="text-gray-600 hover:text-indigo-600 text-sm transition-colors">Blog</a></li>
          <li><a href="/community" class="text-gray-600 hover:text-indigo-600 text-sm transition-colors">Community</a></li>
        </ul>
      </div>

      <!-- Support -->
      <div>
        <h4 class="text-sm font-semibold text-gray-900 mb-4">Support</h4>
        <ul class="space-y-3">
          <li><a href="/help" class="text-gray-600 hover:text-indigo-600 text-sm transition-colors">Help</a></li>
          <li><a href="/contact" class="text-gray-600 hover:text-indigo-600 text-sm transition-colors">Contact</a></li>
          <li><a href="/api" class="text-gray-600 hover:text-indigo-600 text-sm transition-colors">API</a></li>
        </ul>
      </div>
    </div>

    <!-- Bottom Section -->
    <div class="border-t border-gray-200 mt-12 pt-8">
      <div class="flex flex-col md:flex-row justify-between items-start md:items-center space-y-4 md:space-y-0">
        <!-- Legal Links -->
        <div class="flex flex-wrap gap-6 text-sm text-gray-600">
          <a href="/privacy" class="hover:text-indigo-600 transition-colors">Privacy Policy</a>
          <a href="/terms" class="hover:text-indigo-600 transition-colors">Terms of Service</a>
          <a href="/cookies" class="hover:text-indigo-600 transition-colors">Cookie Policy</a>
          <a href="/security" class="hover:text-indigo-600 transition-colors">Security</a>
          <a href="/compliance" class="hover:text-indigo-600 transition-colors">Compliance</a>
          <span class="text-gray-400">© 2024 Tar, Inc. All rights reserved.</span>
        </div>

        <!-- Language & Region Selector -->
        <div class="flex items-center space-x-4">
          <button class="flex items-center space-x-2 text-sm text-gray-600 hover:text-indigo-600 transition-colors">
            <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5h12M9 3v2m1.048 9.5A18.022 18.022 0 016.412 9m6.088 9h7M11 21l5-10 5 10M12.751 5C11.783 10.77 8.07 15.61 3 18.129"/>
            </svg>
            <span>English (US)</span>
            <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"/>
