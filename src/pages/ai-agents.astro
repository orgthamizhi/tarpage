---
import Layout from '../layouts/Layout.astro';
import HeaderTarOS from '../components/HeaderTarOS.astro';
import FooterTarOS from '../components/FooterTarOS.astro';
---

<Layout title="AI Agents - Tar">
  <HeaderTarOS />
  <main class="pt-24 pb-20 bg-white">
    <div class="max-w-6xl mx-auto px-6 lg:px-8">
      <!-- Hero Section -->
      <div class="text-center mb-20">
        <div class="text-8xl mb-6">&#129302;</div>
        <h1 class="text-4xl md:text-5xl font-bold text-gray-900 mb-4">
          AI Agents
        </h1>
        <p class="text-lg text-gray-600 max-w-2xl mx-auto">
          Intelligent automation for your business.
        </p>
      </div>

      <!-- AI Network Illustration -->
      <div class="text-center mb-20">
        <div class="max-w-2xl mx-auto">
          <svg viewBox="0 0 600 300" class="w-full h-auto text-purple-600">
            <!-- Central AI Brain -->
            <circle cx="300" cy="150" r="40" fill="currentColor" opacity="0.2"/>
            <circle cx="300" cy="150" r="25" fill="currentColor" opacity="0.4"/>
            <text x="300" y="155" text-anchor="middle" class="text-sm fill-current">AI</text>
            
            <!-- Agent Nodes -->
            <circle cx="150" cy="80" r="25" fill="currentColor" opacity="0.3"/>
            <text x="150" y="85" text-anchor="middle" class="text-xs fill-white">&#128722;</text>
            
            <circle cx="450" cy="80" r="25" fill="currentColor" opacity="0.3"/>
            <text x="450" y="85" text-anchor="middle" class="text-xs fill-white">&#128101;</text>
            
            <circle cx="150" cy="220" r="25" fill="currentColor" opacity="0.3"/>
            <text x="150" y="225" text-anchor="middle" class="text-xs fill-white">&#9881;</text>
            
            <circle cx="450" cy="220" r="25" fill="currentColor" opacity="0.3"/>
            <text x="450" y="225" text-anchor="middle" class="text-xs fill-white">&#128200;</text>
            
            <!-- Connection Lines -->
            <line x1="300" y1="150" x2="150" y2="80" stroke="currentColor" stroke-width="2" opacity="0.3"/>
            <line x1="300" y1="150" x2="450" y2="80" stroke="currentColor" stroke-width="2" opacity="0.3"/>
            <line x1="300" y1="150" x2="150" y2="220" stroke="currentColor" stroke-width="2" opacity="0.3"/>
            <line x1="300" y1="150" x2="450" y2="220" stroke="currentColor" stroke-width="2" opacity="0.3"/>
            
            <!-- Data Flow Animation -->
            <circle cx="225" cy="115" r="3" fill="currentColor" opacity="0.6">
              <animateMotion dur="3s" repeatCount="indefinite">
                <path d="M0,0 L150,70"/>
              </animateMotion>
            </circle>
          </svg>
        </div>
        <p class="text-gray-500 text-sm mt-4">AI agents working together to automate your business</p>
      </div>

      <!-- How AI Works -->
      <div class="text-center mb-20">
        <h2 class="text-2xl font-bold text-gray-900 mb-12">How it works</h2>
        <div class="grid grid-cols-1 md:grid-cols-4 gap-8">
          <div class="text-center">
            <div class="text-4xl mb-3">&#128065;</div>
            <h3 class="font-semibold text-gray-900 mb-1">Watch</h3>
            <p class="text-gray-600 text-sm">Monitor everything</p>
          </div>
          <div class="text-center">
            <div class="text-4xl mb-3">&#129302;</div>
            <h3 class="font-semibold text-gray-900 mb-1">Think</h3>
            <p class="text-gray-600 text-sm">Analyze patterns</p>
          </div>
          <div class="text-center">
            <div class="text-4xl mb-3">&#9889;</div>
            <h3 class="font-semibold text-gray-900 mb-1">Act</h3>
            <p class="text-gray-600 text-sm">Take action</p>
          </div>
          <div class="text-center">
            <div class="text-4xl mb-3">&#128200;</div>
            <h3 class="font-semibold text-gray-900 mb-1">Improve</h3>
            <p class="text-gray-600 text-sm">Get better</p>
          </div>
        </div>
      </div>

      <!-- Business Management -->
      <section class="mb-20">
        <div class="text-center mb-12">
          <h2 class="text-2xl font-bold text-gray-900 mb-4">Business Management</h2>
          <p class="text-gray-600">Automated operations and optimization</p>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
          <!-- Inventory -->
          <div class="bg-blue-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M20 7l-8-4-8 4m16 0l-8 4m8-4v10l-8 4m0-10L4 7m8 4v10M4 7v10l8 4"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4">Inventory</h3>
            <p class="text-gray-600">Auto-reordering and stock optimization</p>
          </div>

          <!-- Pricing -->
          <div class="bg-green-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-green-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4">Pricing</h3>
            <p class="text-gray-600">Dynamic pricing and profit optimization</p>
          </div>
        </div>
      </section>

      <!-- Customer Experience -->
      <section class="mb-20">
        <div class="text-center mb-12">
          <h2 class="text-2xl font-bold text-gray-900 mb-4">Customer Experience</h2>
          <p class="text-gray-600">Personalized and automated support</p>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
          <!-- Personalization -->
          <div class="bg-purple-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-purple-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4">Personalization</h3>
            <p class="text-gray-600">Custom recommendations and campaigns</p>
          </div>

          <!-- Support -->
          <div class="bg-yellow-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-yellow-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 12h.01M12 12h.01M16 12h.01M21 12c0 4.418-4.03 8-9 8a9.863 9.863 0 01-4.255-.949L3 20l1.395-3.72C3.512 15.042 3 13.574 3 12c0-4.418 4.03-8 9-8s9 3.582 9 8z"/>
            </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4">Support</h3>
            <p class="text-gray-600">24/7 intelligent assistance</p>
          </div>
        </div>
      </section>

      <!-- Operations Optimization -->
      <section class="mb-20">
        <div class="text-center mb-12">
          <div class="text-8xl mb-6">&#9881;</div>
          <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-4">Operations Optimization</h2>
          <p class="text-lg text-gray-600 max-w-2xl mx-auto">Streamlined logistics and resource management</p>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
          <!-- Delivery & Logistics -->
          <div class="bg-indigo-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 20l-5.447-2.724A1 1 0 013 16.382V5.618a1 1 0 011.447-.894L9 7m0 13l6-3m-6 3V7m6 10l4.553 2.276A1 1 0 0021 18.382V7.618a1 1 0 00-.553-.894L15 4m0 13V4m0 0L9 7"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4 leading-tight">Delivery & Logistics</h3>
            <p class="text-gray-600 leading-relaxed mb-4">Real-time route optimization and cost reduction</p>
            <div class="space-y-2 text-sm text-gray-500">
              <div>• Real-time route optimization</div>
              <div>• Automated driver assignment</div>
              <div>• Delivery time prediction and cost reduction</div>
            </div>
          </div>

          <!-- Resource Management -->
          <div class="bg-red-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-red-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197m13.5-9a2.5 2.5 0 11-5 0 2.5 2.5 0 015 0z"/>
            </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4 leading-tight">Resource Management</h3>
            <p class="text-gray-600 leading-relaxed mb-4">Staff scheduling and workload optimization</p>
            <div class="space-y-2 text-sm text-gray-500">
              <div>• Intelligent staff scheduling</div>
              <div>• Workload balancing and performance monitoring</div>
              <div>• Capacity planning and optimization</div>
            </div>
          </div>
        </div>
      </section>

      <!-- Predictive Analytics & Insights -->
      <section class="mb-20">
        <div class="text-center mb-12">
          <div class="text-8xl mb-6">&#128202;</div>
          <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-4">Predictive Analytics & Insights</h2>
          <p class="text-lg text-gray-600 max-w-2xl mx-auto">Future-focused intelligence and growth opportunities</p>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
          <!-- Business Intelligence -->
          <div class="bg-blue-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4 leading-tight">Business Intelligence</h3>
            <p class="text-gray-600 leading-relaxed mb-4">Revenue forecasting and market trend analysis</p>
            <div class="space-y-2 text-sm text-gray-500">
              <div>• Revenue forecasting and market analysis</div>
              <div>• Customer lifetime value prediction</div>
              <div>• Risk assessment and mitigation</div>
            </div>
          </div>

          <!-- Growth Opportunities -->
          <div class="bg-green-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-green-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7h8m0 0v8m0-8l-8 8-4-4-6 6"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4 leading-tight">Growth Opportunities</h3>
            <p class="text-gray-600 leading-relaxed mb-4">Market identification and expansion strategies</p>
            <div class="space-y-2 text-sm text-gray-500">
              <div>• New market identification</div>
              <div>• Product development insights</div>
              <div>• Investment and expansion optimization</div>
            </div>
          </div>
        </div>
      </section>

      <!-- Why AI Agents Transform Business -->
      <section class="mb-20">
        <div class="text-center mb-12">
          <div class="text-8xl mb-6">&#128640;</div>
          <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-4">Why AI Agents Transform Business</h2>
          <p class="text-lg text-gray-600 max-w-2xl mx-auto">The fundamental advantages of AI-powered automation</p>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
          <!-- 24/7 Operations -->
          <div class="bg-purple-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-purple-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4 leading-tight">24/7 Operations</h3>
            <p class="text-gray-600 leading-relaxed mb-4">Never sleep, always optimizing your business</p>
            <div class="text-sm text-gray-500">Continuous monitoring and optimization without human intervention</div>
          </div>

          <!-- Data-Driven Decisions -->
          <div class="bg-yellow-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-yellow-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4 leading-tight">Data-Driven Decisions</h3>
            <p class="text-gray-600 leading-relaxed mb-4">Every action backed by real-time analytics</p>
            <div class="text-sm text-gray-500">Eliminate guesswork with AI-powered insights and recommendations</div>
          </div>

          <!-- Scalable Growth -->
          <div class="bg-indigo-50 p-8 rounded-lg border border-gray-100">
            <div class="w-16 h-16 bg-white rounded-lg flex items-center justify-center mb-6 border border-gray-200">
              <svg class="w-8 h-8 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7h8m0 0v8m0-8l-8 8-4-4-6 6"/>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-gray-900 mb-4 leading-tight">Scalable Growth</h3>
            <p class="text-gray-600 leading-relaxed mb-4">Automatically adapt as your business grows</p>
            <div class="text-sm text-gray-500">AI agents scale with your business without additional overhead</div>
          </div>
        </div>
      </section>

      <!-- AI Agent Benefits - Notion Style -->
      <div class="mb-20">
        <section class="border-l-4 border-indigo-500 pl-8">
          <h2 class="text-3xl font-bold text-gray-900 mb-2">Why AI Agents Transform Business</h2>
          <p class="text-gray-500 text-sm mb-8">The fundamental advantages of AI-powered automation</p>
          
          <div class="space-y-6">
            <div>
              <h3 class="text-xl font-bold text-gray-900 mb-1">24/7 Operations</h3>
              <p class="text-gray-600 text-sm mb-2">Never sleep, always optimizing your business</p>
              <div class="text-gray-500 text-xs">Continuous monitoring and optimization without human intervention</div>
            </div>
            
            <div>
              <h3 class="text-xl font-bold text-gray-900 mb-1">Data-Driven Decisions</h3>
              <p class="text-gray-600 text-sm mb-2">Every action backed by real-time analytics</p>
              <div class="text-gray-500 text-xs">Eliminate guesswork with AI-powered insights and recommendations</div>
            </div>
            
            <div>
              <h3 class="text-xl font-bold text-gray-900 mb-1">Scalable Growth</h3>
              <p class="text-gray-600 text-sm mb-2">Automatically adapt as your business grows</p>
              <div class="text-gray-500 text-xs">AI agents scale with your business without additional overhead</div>
            </div>
          </div>
        </section>
      </div>

      <!-- Benefits -->
      <div class="text-center mb-20">
        <h2 class="text-2xl font-bold text-gray-900 mb-12">AI Benefits</h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
          <div class="text-center">
            <div class="text-4xl mb-4">&#9889;</div>
            <h3 class="font-semibold text-gray-900 mb-2">24/7 Active</h3>
            <p class="text-gray-600 text-sm">Always working, never stops</p>
          </div>
          <div class="text-center">
            <div class="text-4xl mb-4">&#128200;</div>
            <h3 class="font-semibold text-gray-900 mb-2">Data-Driven</h3>
            <p class="text-gray-600 text-sm">Smart decisions, real insights</p>
          </div>
          <div class="text-center">
            <div class="text-4xl mb-4">&#128640;</div>
            <h3 class="font-semibold text-gray-900 mb-2">Auto-Scale</h3>
            <p class="text-gray-600 text-sm">Grows with your business</p>
          </div>
        </div>
      </div>

      <!-- Simple Illustration -->
      <div class="text-center mb-20">
        <div class="max-w-md mx-auto">
          <svg viewBox="0 0 400 200" class="w-full h-auto text-purple-600">
            <!-- AI Brain illustration -->
            <circle cx="200" cy="100" r="60" fill="currentColor" opacity="0.1"/>
            <circle cx="200" cy="100" r="40" fill="currentColor" opacity="0.2"/>
            <circle cx="200" cy="100" r="20" fill="currentColor" opacity="0.3"/>
            
            <!-- Connected nodes -->
            <circle cx="120" cy="60" r="15" fill="currentColor" opacity="0.4"/>
            <circle cx="280" cy="60" r="15" fill="currentColor" opacity="0.4"/>
            <circle cx="120" cy="140" r="15" fill="currentColor" opacity="0.4"/>
            <circle cx="280" cy="140" r="15" fill="currentColor" opacity="0.4"/>
            
            <!-- Connection lines -->
            <line x1="200" y1="100" x2="120" y2="60" stroke="currentColor" stroke-width="2" opacity="0.3"/>
            <line x1="200" y1="100" x2="280" y2="60" stroke="currentColor" stroke-width="2" opacity="0.3"/>
            <line x1="200" y1="100" x2="120" y2="140" stroke="currentColor" stroke-width="2" opacity="0.3"/>
            <line x1="200" y1="100" x2="280" y2="140" stroke="currentColor" stroke-width="2" opacity="0.3"/>
          </svg>
        </div>
        <p class="text-gray-500 text-sm mt-4">AI agents working together to manage your business</p>
      </div>

      <!-- CTA -->
      <div class="text-center bg-gray-50 rounded-2xl p-12">
        <div class="text-6xl mb-4">&#129302;</div>
        <h2 class="text-2xl font-bold text-gray-900 mb-4">Experience AI-Powered Business</h2>
        <p class="text-gray-600 mb-8">Let AI agents transform how you operate</p>
        <button class="bg-purple-600 hover:bg-purple-700 text-white px-8 py-3 rounded-xl font-medium transition-colors">
          Activate AI Agents
        </button>
      </div>
    </div>
  </main>
  <FooterTarOS />
</Layout>